{
  "$id": "https://denidin.example/schemas/mcp/receipt_parse.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Parse Receipt Request/Response",
  "type": "object",
  "x-http": {
    "method": "POST",
    "path": "/documents/parse",
    "notes": "Internal parse endpoint (provider examples use webhooks/callbacks; this is our parse contract)."
  },
  "properties": {
    "file_url": {"type": "string", "format": "uri"},
    "receipt_id": {"type": "string"}
  },
  "required": ["file_url"],
  "response_example": {
    "id": "123",
    "status": "parsed",
    "parsed_metadata": {"amount": 12.34, "merchant": "Example"}
  }
}
